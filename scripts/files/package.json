{
  "name": "my-name-application",
  "description": "This repository is a starter project that use React Toolkit Axa.",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "tsc && vite build -m production.local && serve -s build -p 5002",
    "test": "vitest run",
    "test:dev": "vitest",
    "cover:local": "vitest run --coverage",
    "cover": "vitest run --coverage --silent",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "check": "npm run prettier && npm run lint",
    "fix": "npm run prettier:fix && npm run lint:fix",
    "prepare": "husky install",
    "cz": "cz"
  },
  "dependencies": {
    "@axa-fr/react-oidc": "6.20.0",
    "@axa-fr/react-toolkit-all": "2.0.0",
    "@axa-fr/react-toolkit-core": "2.0.0",
    "@popperjs/core": "2.11.8",
    "@tanstack/react-query": "4.29.14",
    "@tanstack/react-query-devtools": "4.29.14",
    "@uiw/react-textarea-code-editor": "2.1.6",
    "cross-fetch": "3.1.5",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.44.3",
    "react-router-dom": "6.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash": "4.14.195",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/coverage-c8": "0.26.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.42.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest-cucumber": "3.0.1",
    "jsdom": "22.1.0",
    "lint-staged": "13.2.2",
    "msw": "1.2.2",
    "prettier": "2.8.8",
    "rollup-plugin-visualizer": "5.9.2",
    "sass": "1.63.4",
    "serve": "14.2.0",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.26.1",
    "vitest-sonar-reporter": "0.4.1",
    "whatwg-fetch": "3.6.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
