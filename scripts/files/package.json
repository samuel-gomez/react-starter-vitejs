{
  "name": "my-name-application",
  "description": "This repository is a starter project that use React Toolkit Axa.",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "tsc && vite build -m production.local && serve -s build -p 5002",
    "test": "vitest run",
    "test:dev": "vitest",
    "cover:local": "vitest run --coverage",
    "cover": "vitest run --coverage --silent",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "check": "npm run prettier && npm run lint",
    "fix": "npm run prettier:fix && npm run lint:fix",
    "prepare": "husky install",
    "cz": "cz"
  },
  "dependencies": {
    "@axa-fr/react-oidc": "7.9.1",
    "@axa-fr/react-toolkit-all": "2.0.0",
    "@axa-fr/react-toolkit-core": "2.0.0",
    "@popperjs/core": "2.11.8",
    "@tanstack/react-query": "5.8.4",
    "@tanstack/react-query-devtools": "5.8.4",
    "cross-fetch": "4.0.0",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.48.2",
    "react-router-dom": "6.19.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.2",
    "@commitlint/config-conventional": "18.4.2",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.1.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.5.1",
    "@types/jest-axe": "3.5.8",
    "@types/lodash": "4.14.201",
    "@types/react": "18.2.19",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "@vitejs/plugin-react": "4.2.0",
    "@vitest/coverage-v8": "0.34.6",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.54.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest-axe": "8.0.0",
    "jest-cucumber": "3.0.1",
    "jsdom": "22.1.0",
    "lint-staged": "15.1.0",
    "msw": "1.3.2",
    "prettier": "3.1.0",
    "rollup-plugin-visualizer": "5.9.2",
    "sass": "1.69.5",
    "serve": "14.2.1",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6",
    "vitest-sonar-reporter": "0.5.0",
    "whatwg-fetch": "3.6.19"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
