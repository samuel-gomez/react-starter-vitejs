{
  "apiUrl": {
    "base": "https://react-starter-api.vercel.app/api/",
    "github": "https://raw.githubusercontent.com/"
  },
  "fetchConfig": {
    "headers": {
      "Content-type": "application/json; charset=UTF-8"
    }
  },
  "oidc": {
