{
  "apiUrl": {
    "base": "https://react-starter-api.vercel.app/api/",
    "github": "https://raw.githubusercontent.com/"
  },
  "baseUrl": "http://localhost:5002/",
  "fetchConfig": {
    "headers": {
      "Content-type": "application/json; charset=UTF-8"
    }
  },
  "oidc": {
    "isEnabled": true,
    "client_id": "interactive.public",
    "redirect_uri": "http://localhost:5002/authentication/callback",
    "response_type": "code",
    "post_logout_redirect_uri": "http://localhost:5002/",
    "scope": "openid profile email api offline_access",
    "authority": "https://demo.duendesoftware.com",
    "silent_redirect_uri": "http://localhost:5002/authentication/silent_callback",
    "automaticSilentRenew": true,
    "loadUserInfo": true
  }
}
