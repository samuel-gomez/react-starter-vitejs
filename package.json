{
  "name": "react-starter-vitejs",
  "description": "This repository is a starter project that use React Toolkit Axa and Vite.",
  "version": "0.5.4",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build && npm run copy",
    "preview": "tsc && vite build -m production.local && npm run copy && serve -s build -p 5002",
    "test": "vitest run",
    "test:dev": "vitest",
    "cover:local": "vitest run --coverage",
    "cover": "vitest run --coverage --silent",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "check": "npm run prettier && npm run lint",
    "fix": "npm run prettier:fix && npm run lint:fix",
    "prepare": "husky install",
    "cz": "cz",
    "clean": "cd ./scripts && npm i && npm start && cd .. && npx rimraf ./scripts",
    "copy": "cd ./scripts && npm i && npm run copy"
  },
  "dependencies": {
    "@axa-fr/react-oidc": "7.3.4",
    "@axa-fr/react-toolkit-all": "2.0.0",
    "@axa-fr/react-toolkit-core": "2.0.0",
    "@popperjs/core": "2.11.8",
    "@tanstack/react-query": "4.33.0",
    "@tanstack/react-query-devtools": "4.33.0",
    "@uiw/react-textarea-code-editor": "2.1.7",
    "axe-core": "4.7.2",
    "cross-fetch": "4.0.0",
    "downloadjs": "1.4.7",
    "github-markdown-css": "5.2.0",
    "lodash": "4.17.21",
    "prism-react-renderer": "2.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-draggable": "4.4.5",
    "react-hook-form": "7.45.4",
    "react-live": "4.1.3",
    "react-markdown": "8.0.7",
    "react-router-dom": "6.13.0",
    "rehype-slug": "5.1.0",
    "remark-gfm": "3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/downloadjs": "1.4.3",
    "@types/lodash": "4.14.197",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/coverage-c8": "0.26.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.48.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest-cucumber": "3.0.1",
    "jsdom": "22.1.0",
    "lint-staged": "14.0.1",
    "msw": "1.2.5",
    "prettier": "3.0.3",
    "rollup-plugin-visualizer": "5.9.2",
    "sass": "1.66.1",
    "serve": "14.2.1",
    "typescript": "5.2.2",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.26.1",
    "vitest-sonar-reporter": "0.4.1",
    "whatwg-fetch": "3.6.18"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
