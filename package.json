{
  "name": "react-starter-vitejs",
  "description": "This repository is a starter project that use React Toolkit Axa and CRA.",
  "version": "0.5.3",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "tsc && vite build -m production.local && serve -s build -p 5002",
    "test": "vitest run",
    "test:dev": "vitest",
    "cover:local": "vitest run --coverage",
    "cover": "vitest run --coverage --silent",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "check": "npm run prettier && npm run lint",
    "fix": "npm run prettier:fix && npm run lint:fix",
    "prepare": "husky install",
    "cz": "cz",
    "clean": "cd ./scripts && npm i && npm start && cd .. && npx rimraf ./scripts"
  },
  "dependencies": {
    "@axa-fr/react-oidc": "6.11.1",
    "@axa-fr/react-toolkit-all": "2.0.0",
    "@axa-fr/react-toolkit-core": "2.0.0",
    "@popperjs/core": "2.11.6",
    "@tanstack/react-query": "4.20.4",
    "@tanstack/react-query-devtools": "4.20.4",
    "@uiw/react-textarea-code-editor": "2.0.6",
    "axe-core": "4.6.1",
    "cross-fetch": "3.1.5",
    "downloadjs": "1.4.7",
    "github-markdown-css": "5.1.0",
    "lodash": "4.17.21",
    "prism-react-renderer": "2.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-draggable": "4.4.5",
    "react-hook-form": "7.41.2",
    "react-live": "2.4.1",
    "react-markdown": "8.0.4",
    "react-router-dom": "6.5.0",
    "rehype-slug": "5.1.0",
    "remark-gfm": "3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/downloadjs": "1.4.3",
    "@types/lodash": "4.14.191",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "@vitejs/plugin-react": "3.0.0",
    "@vitest/coverage-c8": "0.26.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.30.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.0",
    "jest-cucumber": "3.0.1",
    "jsdom": "20.0.3",
    "lint-staged": "13.1.0",
    "msw": "0.49.2",
    "prettier": "2.8.1",
    "rollup-plugin-visualizer": "5.9.0",
    "sass": "1.57.1",
    "serve": "14.1.2",
    "typescript": "4.9.3",
    "vite": "4.0.5",
    "vite-tsconfig-paths": "4.0.3",
    "vitest": "0.26.1",
    "vitest-sonar-reporter": "0.3.3",
    "whatwg-fetch": "3.6.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
